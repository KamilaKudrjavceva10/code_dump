#include <iostream>

#include <string>
using namespace std;
class Animal {
public:
    void voice()
    {
        cout << "voice";
    }
};

class Dog : public Animal {
public:
    void voice()
    {
        cout << "Dog - woof" << endl;
    }
};
class Cat : public Animal {
public:
    void voice()
    {
        cout << "Cat - meow" << endl;
    }
};
class Duck : public Animal {
public:
    void voice()
    {
        cout << "Duck - quack" << endl;
    }
};
int main() {
    Dog a;
    a.voice();
    Cat s;
    s.voice();
    Duck d;
    d.voice();
    return 0;
}
