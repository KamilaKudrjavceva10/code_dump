#include <iostream>
#include <cmath>
using namespace std;

int main()
{
	double a, b, c, D, x1, x2,xv,yv;
	cout << "Enter please coefficients"<<endl ;
	cin >> a >> b >> c;
	D = b * b - 4 * a * c;
	if (D < 0) { cout << "D<0"; exit(0); }
	x1 = (-b + sqrt(D)) / (2 * a);
	x2 = (-b - sqrt(D)) / (2 * a);
	xv= (-b ) / (2 * a);
	yv = a * xv * xv + b * xv + c;
	cout << x1 << endl << x2 << endl << xv << endl << yv;
	return 0;
}
